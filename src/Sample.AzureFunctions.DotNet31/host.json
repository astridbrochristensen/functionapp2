{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": 500,
      "visibilityTimeout": "00:00:02",
      "maxDequeueCount": 3,
      "batchSize": 16,
      "newBatchThreshold": 8
    },
    "serviceBus": {
      "maxConcurrentCalls": 1,
      "prefetchCount": 100,
      "autoRenewTimeout": "00:05:00"
    },
    "http": {
      "maxConcurrentRequests": 10,
      "routePrefix": "test"
    }
  }
}